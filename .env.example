# Required API Keys
FORGE_API_KEY=your_forge_api_key_here
GITHUB_TOKEN=your_github_token_here

# SPT & Fika Versions
SPT_VERSION=latest
FIKA_VERSION=latest

# Public URL Configuration
# Set this to your domain if using reverse proxy (nginx, cloudflare, etc.)
# Example: https://tarkhub.mydomain.com
PUBLIC_URL=https://localhost:6969

# Enable if behind reverse proxy (nginx, traefik, etc.)
# Set to "true" if using nginx or other reverse proxy
ENABLE_PROXY_HEADERS=false